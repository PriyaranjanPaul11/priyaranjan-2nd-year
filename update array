#include <iostream>
using namespace std;

int main() {
    
    int arr[10]={12,23,34,43,45,56,65,76,67,87};
    
    int index=4;
    int newValue=11;
    
    arr[index]=newValue;
    cout<<"updated array:"<<endl;

    
    for(int i=0;i<10;i++){
        cout<<arr[i]<<" ";
    }
    
    cout<<endl;

    return 0;
}
