#include <iostream>
using namespace std;

int main() {
    
    int array[12]={1,2,4,312,34,54,4,5,7,6,89,9};
    
    int n=12;
    
    cout<<"Print the Array"<<endl;
    
    for(int i=0;i<n;i++){
        
        cout<<array[i]<<" ";
    }
    cout<<endl<<"Everything is fine"<<endl<<endl;
    
    cout<<"Value at 3 index"<<endl<<array[3]<<endl;
    

    return 0;
}
